{
    "manifest_version": 3,
    "name": "arpeely-frontend-home-exercise",
    "description": "Made by Naaman Frenkel for arpeely",
    "version": "1.0.0",
    "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>",
      "storage"
  ],
    "action": { "default_popup": "index.html" },
    "icons": {
      "16": "icons/icon.png",
      "32": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }